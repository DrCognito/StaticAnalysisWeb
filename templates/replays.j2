{% set provider='team'%}
{% extends "team.j2" %}
{% block vis %}
{% if time_string is not none %}
Data range: {{time_string}}
<h2>Replays</h2>
{% endif %}
{% if replays is not none %}
    <table>
        <td style="min-width:80px">Dire</td><td style="min-width:80px">Radiant</td>
        {% for r_pair in replays %}
            {% set left = '' if r_pair[0] is none else r_pair[0] %}
            {% set right = '' if r_pair[1] is none else r_pair[1] %}
            <tr>
            <td align='right'>{{left}}</td>
            <td align='right'>{{right}}</td>
            </tr>
        {% endfor %}
    </table>
{% endif %}
{% if drafts_only is not none %}
<h2>Drafts Only</h2>
    <table>
        <td style="min-width:80px">Dire</td><td style="min-width:80px">Radiant</td>
        {% for r_pair in drafts_only %}
            {% set left = '' if r_pair[0] is none else r_pair[0] %}
            {% set right = '' if r_pair[1] is none else r_pair[1] %}
            <tr>
            <td align='right'>{{left}}</td>
            <td align='right'>{{right}}</td>
            </tr>
        {% endfor %}
    </table>
{% endif %}
{% if winrates is not none %}
<h2>Win Rates</h2>
{{winrates}}
{% endif %}
{% endblock vis %}